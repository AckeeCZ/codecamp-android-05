        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="cz.ackee.codecamp05.ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="cz.ackee.codecamp05.ui.activity.FilesActivity"/>

        <activity android:name="cz.ackee.codecamp05.ui.activity.SharedPreferencesActivity"/>


        <activity android:name="cz.ackee.codecamp05.ui.activity.RetainFragmentActivity"/>
        <activity android:name="cz.ackee.codecamp05.ui.activity.DatabaseActivity"/>


    </application>

</manifest>
